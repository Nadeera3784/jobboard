{
  "name": "backend",
  "version": "0.0.1",
  "description": "JobBoard Backend API",
  "author": "Nadeera Sampath",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "cron": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/cron.js\"",
    "cli": "ts-node src/cli.ts",
    "debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^5.17.1",
    "@bull-board/express": "^5.17.1",
    "@bull-board/nestjs": "^5.17.1",
    "@elastic/elasticsearch": "^9.0.3",
    "@google/generative-ai": "^0.11.2",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mongoose": "^10.0.4",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^5.1.2",
    "aws-sdk": "^2.1590.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cron": "^3.1.6",
    "deep-parse-json": "^2.0.0",
    "hbs": "^4.2.0",
    "helmet": "^7.1.0",
    "hibp": "^14.1.1",
    "ip-address": "^9.0.5",
    "moment": "^2.30.1",
    "mongoose": "^8.2.0",
    "morgan": "^1.10.0",
    "node-mocks-http": "^1.14.1",
    "openai": "^4.51.0",
    "otplib": "^12.0.1",
    "password-validator": "^5.3.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "resend": "^3.2.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "tsc-watch": "^6.2.0",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
